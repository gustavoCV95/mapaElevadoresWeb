# .env.example - Exemplo de configuração
# Copie para .env e configure com valores reais

# URLs das Planilhas Google Sheets
PLANILHA_URL="https://docs.google.com/spreadsheets/d/SEU_ID_AQUI/edit"
PLANILHA_KPIS_URL="https://docs.google.com/spreadsheets/d/SEU_ID_AQUI/edit"

# Configurações de Cache
CACHE_TIMEOUT=300

# Configurações de Segurança
FLASK_SECRET_KEY=sua-chave-secreta-super-segura-aqui
MAX_TENTATIVAS_LOGIN=5
BLOQUEIO_TEMPO=900

# Usuários (formato: nome:senha)
USUARIO_1=usuario1:senha1
USUARIO_2=usuario2:senha2

# Ambiente
FLASK_ENV=development